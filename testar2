resistencias = []

prim_resis = float(input("Primeira: "))
quantas = int(input("Quantas: "))
r_Eq = prim_resis

for i in range (quantas):
    valor_resis= float(input("Valor da resis: "))
    resistencias.append(valor_resis)
    r_Eq = (r_Eq * resistencias[i]) / (r_Eq + resistencias[i])
rend = float(input("Rend: "))
