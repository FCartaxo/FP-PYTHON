import numpy as np

arr = np.array([[1, 1, 1], [2, -3, 0], [0, -3, 3]])
arr2 = np.array([0, 2, 3])

arr_inv = np.linalg.inv(arr)   #calculo matriz inversa

resultado = arr_inv.dot(arr2)  #calculo do resultado

print(arr_inv)
